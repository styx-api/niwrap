{
  "name": "spec-file-merge",
  "author": "Connectome Workbench Developers",
  "command-line": "wb_command -spec-file-merge [SPEC_1] [SPEC_2] [OUT_SPEC]",
  "description": "Merge two spec files into one.\n\nThe output spec file contains every file that is in either of the input spec files.",
  "schema-version": "0.5+styx",
  "inputs": [
    {
      "id": "spec_1",
      "name": "spec_1",
      "description": "first spec file to merge",
      "optional": false,
      "value-key": "[SPEC_1]",
      "type": "String"
    },
    {
      "id": "spec_2",
      "name": "spec_2",
      "description": "second spec file to merge",
      "optional": false,
      "value-key": "[SPEC_2]",
      "type": "String"
    },
    {
      "id": "out_spec",
      "name": "out_spec",
      "description": "output - output spec file",
      "optional": false,
      "value-key": "[OUT_SPEC]",
      "type": "String"
    }
  ],
  "url": "https://github.com/Washington-University/workbench"
}

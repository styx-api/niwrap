      "id": "start_surface",
      "name": "Start Surface"
    },
    {
      "description": "The ending surface file.",
      "value-key": "[END_SURFACE]",
      "type": "File",
      "optional": false,
      "id": "end_surface",
      "name": "End Surface"
    },
    {
      "description": "The warp field file with .m3z extension.",
      "value-key": "[WARP_FIELD]",
      "type": "File",
      "optional": false,
      "id": "warp_field",
      "name": "Warp Field"
    }
  ],
  "author": "FreeSurfer Developers",
  "url": "https://github.com/freesurfer/freesurfer"

{
  "name": "cor_to_minc",
  "description": "Converts a directory of COR files to a MINC file.",
  "command-line": "cor_to_minc [COR_DIRECTORY] [MINC_FILE]",
  "schema-version": "0.5+styx",
  "inputs": [
    {
      "description": "Directory containing COR files",
      "value-key": "[COR_DIRECTORY]",
      "type": "String",
      "optional": false,
      "id": "cor_directory",
      "name": "COR Directory"
    },
    {
      "description": "Output MINC file name",
      "value-key": "[MINC_FILE]",
      "type": "String",
      "optional": false,
      "id": "minc_file",
      "name": "MINC File Name"
    }
  ],
  "output-files": [
    {
      "path-template": "[MINC_FILE]",
      "description": "Converted MINC file",
      "id": "output_minc",
      "name": "Output MINC File"
    }
  ],
  "author": "FreeSurfer Developers",
  "url": "https://github.com/freesurfer/freesurfer"
}

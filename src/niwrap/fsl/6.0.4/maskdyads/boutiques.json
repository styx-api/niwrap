{
  "name": "maskdyads",
  "author": "FMRIB Analysis Group, University of Oxford",
  "description": "Tool to mask dyads with threshold",
  "command-line": "maskdyads [DYADS] [FSAMPLES] [THRESHOLD]",
  "inputs": [
    {
      "description": "Input dyads file",
      "value-key": "[DYADS]",
      "type": "File",
      "optional": false,
      "id": "dyads",
      "name": "Dyads file"
    },
    {
      "description": "Input fsamples file",
      "value-key": "[FSAMPLES]",
      "type": "File",
      "optional": false,
      "id": "fsamples",
      "name": "Fsamples file"
    },
    {
      "command-line-flag": "[THR]",
      "description": "Threshold (default is 0.05)",
      "value-key": "[THRESHOLD]",
      "type": "Number",
      "optional": true,
      "id": "threshold",
      "name": "Threshold"
    }
  ],
  "schema-version": "0.5+styx",
  "url": "https://fsl.fmrib.ox.ac.uk/fsl/fslwiki"
}

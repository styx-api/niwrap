{
  "tool-version": "7.4.1",
  "name": "fixup_mni_paths",
  "author": "FreeSurfer Developers",
  "description": "A utility for patching MNI tools to ensure correct paths are used.",
  "command-line": "fixup_mni_paths [VERBOSE]",
  "schema-version": "0.5",
  "inputs": [
    {
      "command-line-flag": "-v",
      "description": "Provide verbose output during the patch process",
      "value-key": "[VERBOSE]",
      "type": "Flag",
      "id": "verbose",
      "name": "Verbose Flag"
    }
  ],
  "output-files": [
    {
      "path-template": "autocrop.old",
      "description": "Original file backup for autocrop",
      "id": "autocrop_backup",
      "name": "Autocrop Backup"
    },
    {
      "path-template": "field2imp.old",
      "description": "Original file backup for field2imp",
      "id": "field2imp_backup",
      "name": "Field2Imp Backup"
    },
    {
      "path-template": "imp2field.old",
      "description": "Original file backup for imp2field",
      "id": "imp2field_backup",
      "name": "Imp2Field Backup"
    },
    {
      "path-template": "make_template.old",
      "description": "Original file backup for make_template",
      "id": "make_template_backup",
      "name": "Make Template Backup"
    },
    {
      "path-template": "mritoself.old",
      "description": "Original file backup for mritoself",
      "id": "mritoself_backup",
      "name": "MRIToSelf Backup"
    },
    {
      "path-template": "mritotal.old",
      "description": "Original file backup for mritotal",
      "id": "mritotal_backup",
      "name": "MRITotal Backup"
    },
    {
      "path-template": "nu_correct.old",
      "description": "Original file backup for nu_correct",
      "id": "nu_correct_backup",
      "name": "NU Correct Backup"
    },
    {
      "path-template": "nu_estimate.old",
      "description": "Original file backup for nu_estimate",
      "id": "nu_estimate_backup",
      "name": "NU Estimate Backup"
    },
    {
      "path-template": "nu_estimate_np_and_em.old",
      "description": "Original file backup for nu_estimate_np_and_em",
      "id": "nu_estimate_np_and_em_backup",
      "name": "NU Estimate NP and EM Backup"
    },
    {
      "path-template": "nu_evaluate.old",
      "description": "Original file backup for nu_evaluate",
      "id": "nu_evaluate_backup",
      "name": "NU Evaluate Backup"
    },
    {
      "path-template": "resample_labels.old",
      "description": "Original file backup for resample_labels",
      "id": "resample_labels_backup",
      "name": "Resample Labels Backup"
    },
    {
      "path-template": "sharpen_volume.old",
      "description": "Original file backup for sharpen_volume",
      "id": "sharpen_volume_backup",
      "name": "Sharpen Volume Backup"
    },
    {
      "path-template": "xfmtool.old",
      "description": "Original file backup for xfmtool",
      "id": "xfmtool_backup",
      "name": "XFMTool Backup"
    }
  ],
  "container-image": {
    "type": "docker",
    "image": "freesurfer/freesurfer:7.4.1"
  },
  "url": "https://github.com/freesurfer/freesurfer"
}
